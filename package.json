{
    "name": "dxf-viewer-example",
    "description": "Example of dxf-viewer package application",
    "repository": "https://github.com/vagran/dxf-viewer-example",
    "homepage": "https://vagran.github.io/dxf-viewer-example",
    "license": "MIT",
    "version": "1.1.0",
    "dependencies": {
        "vue": "^3.3.4",
        "quasar": "^2.12.0",
        "@quasar/extras": "^1.16.4",
        "dxf-viewer": "../dxf-viewer",
        "three": "^0.152.2"
    },
    "scripts": {
        "serve": "vite",
        "build": "vue-tsc --noEmit && vite build --emptyOutDir --base=/dxf-viewer-example/"
    },
    "debug": {
        "env": {
            "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344"
        }
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^4.2.3",
        "less": "^4.1.3",
        "typescript": "^5.0.4",
        "vite": "^4.3.8",
        "vue-tsc": "^1.6.5",
        "@quasar/vite-plugin": "^1.3.3"
    },
    "keywords": [
        "dxf",
        "viewer",
        "javascript",
        "webgl",
        "html5",
        "example"
    ]
}
